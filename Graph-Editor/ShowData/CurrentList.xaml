<Window x:Class="Graph_Editor.ShowData.CurrentList"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Graph_Editor.ShowData"
        Closing="DataWindow_Closing"
        mc:Ignorable="d" Icon="../images/logo.png"
        Title="Current adjacency matrix" Height="350" Width="600" ResizeMode="NoResize">

    <Grid Background="#98B0B0">
        <Image Source="../images/logo.png" Width="32" Height="32" VerticalAlignment="Bottom" HorizontalAlignment="Right"/>
        <Button Content="Я насмотрелся :)" HorizontalAlignment="Left" Margin="10,273,0,7" Width="162" FontSize="20" Click="Cancel_Click"/>
        <TextBox x:Name="mainBlock" HorizontalAlignment="Left" Height="258" Margin="45,10,0,0" TextWrapping="Wrap"
                 Text=" " VerticalAlignment="Top" Width="539" Background="Transparent" Foreground="Black"
                 FontSize="20"/>
        <TextBox x:Name="sideBlock" HorizontalAlignment="Left"
                 Height="258" Margin="10,10,0,0" TextWrapping="Wrap" Text=""
                 VerticalAlignment="Top" Width="30"
                 Foreground="Black" Background="Transparent"/>
    </Grid>
</Window>
